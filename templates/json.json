    "SCanvas Example": {
        "prefix": "SCanvas Example",
        "body": [
            "\"SCanvas\": {\n    \"ToolTipText\": \"Canvas is a layout widget that allows you to arbitrary position and size child widgets in a relative coordinate space\",\n    \"Slots\": [\n        {\n            \"Position\": [97, 0],\n            \"Size\": [25, 20],\n            \"SButton\": {\"Text\": \"^_^\", \"ButtonColorAndOpacity\": [0, 5, 0 ,1], \"OnClick\": \"print('Head.')\", \"ForegroundColor\": [0,0,1,1]\n            }\n        },\n        {\n            \"Position\": [90, 22],\n            \"Size\": [40, 40],\n            \"SButton\": {\"Text\": \"snow\\n man\", \"ButtonColorAndOpacity\": [0, 2, 0 ,1], \"OnClick\": \"print('I am a snow man.')\", \"ForegroundColor\": [0,0,1,1] }\n        }\n    ]\n}"
        ]
