{
    "SExpandableArea": {
        "prefix": "SExpandableArea",
        "body": [
            "\"SExpandableArea\": {\n    \"HeaderContent\": {\n        $1\n    },\n    \"BodyContent\": {\n        $2\n    }\n}"
        ]
    },
    "SBorder": {
        "prefix": "SBorder",
        "body": [
            "\"SBorder\": {\n    \"BorderImage\": {\n        \"Style\": \"FEditorStyle\",\n        \"Brush\": \"ToolPanel.DarkGroupBorder\"\n    },\n    \"Content\":{\n        $1\n    }\n}"
        ]
    },
    "Chameleon": {
        "prefix": "Chameleon",
        "body": [
            "{\n\t\"TabLabel\": \"A Chameleon Tool\",\n\t\"InitTabSize\": [400, 250],\n\t\"InitTabPosition\": [200, 100],\n    \"InitPyCmd\": \"import $1; $2 = $3.$4.$5(%JsonPath)\",\n    \"Root\": {\n        $6\n    }\n}"
        ]
    },
    "SBox": {
        "prefix": "SBox",
        "body": [
            "\"SBox\":\n{\n    \"MinDesiredWidth\": 200,\n    \"MinDesiredHeight\": 80,\n    \"Content\": {\n        $1\n    }\n}"
        ]
    },
    "SBreadcrumbTrail": {
        "prefix": "SBreadcrumbTrail",
        "body": [
            "\"SBreadcrumbTrail\":\n{\n    \"Aka\": \"$1\",\n    \"type\": \"string\",\n    \"ButtonContentPadding\": 1,\n    \"OnCrumbClick\": \"print(%)\"\n}"
        ]
    },
    "SButtonB": {
        "prefix": "SButtonB",
        "body": [
            "\"SButton\":\n{\n    \"Text\": \"$1\",\n    \"HAlign\": \"Center\",\n    \"VAlign\": \"Center\",\n    \"ContentPadding\": 4,\n    \"ToolTipText\": \"$2\",\n    \"OnClick\": \"print('Button $3 clicked.')\"\n}"
        ]
    },
    "SCanvas": {
        "prefix": "SCanvas",
        "body": [
            "\"SCanvas\": {\n    \"Slots\": [\n        {\n            \"Position\": [95, 0],\n            \"Size\": [80, 20],\n            $1\n        },\n        {\n            $2\n        }\n    ]\n}"
        ]
    },
    "SCheckBox": {
        "prefix": "SCheckBox",
        "body": [
            "\"SCheckBox\":\n{\n    \"Content\": {\n        $1        \n    },\n    \"Padding\": 6,\n    \"IsChecked\": true,\n    \"OnCheckStateChanged\": \"print('checked: %'.format(%))\"\n}"
        ]
    },
    "SColorBlock": {
        "prefix": "SColorBlock",
        "body": [
            "\"SColorBlock\": \n{\n    \"Color\":[$1, $2, $3, $4],\n    \"ColorIsHSV\": false\n}"
        ]
    },
    "SComboBox": {
        "prefix": "SComboBox",
        "body": [
            "\"SComboBox\": {\n    \"Aka\": \"$1\",\n    \"Content\": {\n        \"Text\": \"ComboBox\"\n    },\n    \"OptionsSource\": [\n        \"$2\",\n        \"$3\",\n        \"$4\"\n    ],\n    \"OnSelectionChanged\": \"print(%)\"\n}"
        ]
    },
    "SComboButton": {
        "prefix": "SComboButton",
        "body": [
            "\"SComboButton\": {\n    \"HAlign\": \"Center\",\n    \"ButtonContent\": {\n        \"STextBlock\": {\n            \"Text\": \"Choose And Trigger the Command\"\n        }\n    },\n    \"HasDownArrow\": true,\n    \"OnGetMenuContent\": {\n        \"items\": [\n            {\"name\": \"$1\"          , \"Command\": \"print('$2')\" },\n            {\"name\": \"$3\"          , \"Command\": \"print('$4')\" },\n            {\"name\": \"$5\"          , \"Command\": \"print('$6')\" }\n        ]\n    }\n}"
        ]
    },
    "SDropTarget": {
        "prefix": "SDropTarget",
        "body": [
            "\"SDropTarget\": {\n   \"Text\": \"SDropTarget\",\n   \"HAlign\": \"Center\",\n   \"OnDrop\": \"$1.on_drop_func(%**kwargs)\",\n   \"Content\":\n   {\n        $2\n   }\n}"
        ]
    },
    "SGridPanel": {
        "prefix": "SGridPanel",
        "body": [
            "\"SGridPanel\":\n{\n   \"FillColumn\": [[0, 0.38], [1, 0.62]],\n   \"Slots\":\n   [\n       {\n           \"Column_Row\": [$1, $2],\n           $3\n       }\n    ]\n}"
        ]
    },
    "SHeader": {
        "prefix": "SHeader",
        "body": [
            "\"SHeader\":\n{\n    \"Content\":\n    {\n        $1\n    }\n}"
        ]
    },
    "SHeaderRow": {
        "prefix": "SHeaderRow",
        "body": [
            "\"SHeaderRow\": {\n    \"Columns\": [\n        {\n            \"DefaultLabel\": $1,\n            \"FillWidth\": 1\n        }\n    ]\n}"
        ]
    },
    "SHorizontalBox": {
        "prefix": "SHorizontalBox",
        "body": [
            "\"SHorizontalBox\": \n{\n    \"Slots\": [\n        {\n            $1\n        },\n        {\n            $2\n        }\n    ]\n}"
        ]
    },
    "SHyperlink": {
        "prefix": "SHyperlink",
        "body": [
            "\"SHyperlink\": \n{   \n    \"Style\":\n    {   \n        \"Style\": \"FEditorStyle\",\n        \"StyleName\": \"Credits.Hyperlink\"\n    }, \n    \"Text\": \"$1\"\n}"
        ]
    },
    "SImage": {
        "prefix": "SImage",
        "body": [
            "\"SImage\": {\n    \"Aka\": \"$1\",  \n    \"ImagePathInPlugin\": \"$2\"\n}"
        ]
    },
    "SListView": {
        "prefix": "SListView",
        "body": [
            "\"SListView\": {\n    \"Aka\": \"$1\",\n    \"ListItemsSource\": [\"$2\", \"$3\", \"$4\"],\n    \"SHeaderRow\": {\n        \"Columns\": [\n            {\n                \"DefaultLabel\": \"$5\",\n                \"FillWidth\": 1\n            }\n        ]\n    },\n    \"OnSelectedChanged\": \"print ('Selected: {}  index: {}'.format(%item, %index))\",\n    \"OnMouseButtonDoubleClick\": \"print ('Double click: {}  index: {}'.format(%item, %index))\"\n}"
        ]
    },
    "SListViewMultiColumn": {
        "prefix": "SListViewMultiColumn",
        "body": [
            "\"SListView<MultiColumn>\": {\n    \"ItemHeight\": 6,\n    \"Aka\": \"ListViewLeft\",\n    \"RichText\": true,\n    \"EnableAnimatedScrolling\": false,\n    \"SHeaderRow\": {\n        \"Columns\": [\n            {\n                \"DefaultLabel\": \"Property\",\n                \"FillWidth\": 0.4\n            },\n            {\n                \"DefaultLabel\": \"Value\",\n                \"FillWidth\": 0.6\n            }\n        ]\n    },\n    \"ListItemsSource\": [\n        [\"$1\", \"$2\"]\n        , [\"$3\", \"$4\"]\n    ],\n    \"OnMouseButtonDoubleClick\": \"objectDetailViewer.on_listview_DetailListLeft_mouse_button_double_click(%Index)\",\n    \"OnSelectionChanged\": \"objectDetailViewer.ui_on_listview_DetailList_selection_changed(bRight=False)\"\n}"
        ]
    },
    "SMultiLineEditableTextBox": {
        "prefix": "SMultiLineEditableTextBox",
        "body": [
            "\"SMultiLineEditableTextBox\": \n{\n    \"Text\": \"Muti-line Box\\nLine 1\\nLine 2\\nLine 3\",\n    \"HintText\": \"This is a SMultiLineEditableTextBox\",\n    \"AutoWrapText\": true\n}"
        ]
    },
    "SMultiLineEditableText": {
        "prefix": "SMultiLineEditableText",
        "body": [
            "\"SMultiLineEditableText\":\n{\n    \"Text\": \"line 1 \\nline 2 \\nline 3\",\n    \"HintText\": \"SelectAllTextWhenFocused\",\n    \"Font\": {\n        \"Style\": \"FEditorStyle\",\n        \"StyleName\": \"FontAwesome.16\"\n    },\n    \"Margin\": 10,\n    \"SelectAllTextWhenFocused\": true,\n    \"OnTextChanged\": \"print(%)\",\n    \"OnTextCommitted\": \"print('input text: {}'.format(%))\"\n}"
        ]
    },
    "SOverlay": {
        "prefix": "SOverlay",
        "body": [
            "\"SOverlay\": {\n    \"Slots\":\n    [\n        {\n            \"SImage\": {\n                \"Aka\": \"AImage\",\n                \"ImagePathInPlugin\": \"Resources/PythonIcon_128x.png\"\n            }\n        },\n        {\n            \"Padding\": 10,\n            $1\n        }\n    ]\n}"
        ]
    },
    "SProgressBar": {
        "prefix": "SProgressBar",
        "body": [
            "\"SProgressBar\":\n{\n    \"Aka\": \"$1\",\n    \"FillType\": \"LeftToRight\",\n    \"Percent\": \"0.5\"\n}"
        ]
    },
    "SRichTextBlock": {
        "prefix": "SRichTextBlock",
        "body": [
            "\"SRichTextBlock\":\n{\n    \"Text\": \"$1\"\n}"
        ]
    },
    "SRichTextBlock ExampleA": {
        "prefix": "SRichTextBlock ExampleA",
        "body": [
            "\"SRichTextBlock\": {\n    \"Text\": \"<RichText.red>Red</> <RichText.green>Green</> <RichText.cyan>Rich text Wrapped by</><TextStyle FontFamily=\\\"Roboto\\\" FontSize=\\\"13\\\" FontStyle=\\\"Italic\\\" FontColor=\\\"(R=1, G=1,B=0,A=1)\\\">  'WrapTextAt'</>\\n Torquent nostra lacus pulvinar leo. Facilisi etiam pede dapibus, id nec condimentum mollis gravida Mi fringilla nibh. Ipsum vestibulum, congue venenatis, sem netus. Ipsum duis, class semper Arcu phasellus nulla arcu. Curabitur, quisque. Leo odio ultricies penatibus.\",\n    \"AutoWrapText\": true,\n    \"Justification\": \"Left\",\n    \"LineHeightPercentage\": 1.5,\n    \"Margin\": 30,\n    \"MinDesiredWidth\": 150,\n    \"WrapTextAt\": 200,\n    \"WrappingPolicy\": \"AllowPerCharacterWrapping\",\n    \"Marshaller\": \"ChameleonRichText\"\n}"
        ]
    },
    "SRichTextBlock ExampleB": {
        "prefix": "SRichTextBlock ExampleB",
        "body": [
            "\"SRichTextBlock\": {\n    \"Text\": \"<TextStyle FontFamily=\\\"Roboto\\\" FontSize=\\\"11\\\" FontStyle=\\\"Bold\\\" FontColor=\\\"(R=0,G=1,B=0,A=0.5)\\\">This is a hyperlink: </><a id=\\\"browser\\\" href=\\\"https:\\/\\/www.unrealengine.com/\\\" style=\\\"RichText.Editor.Hyperlink\\\">Unreal Engine</>\",\n    \"Marshaller\": \"ChameleonRichText\"\n}"
        ]
    },
    "SScrollBox": {
        "prefix": "SScrollBox",
        "body": [
            "\"SScrollBox\": {\n    \"Orientation\": \"Vertical\",\n    \"Slots\":\n    [\n        {\n            $1     \n        }\n    ]\n}"
        ]
    },
    "SSearchBox": {
        "prefix": "SSearchBox",
        "body": [
            "\"SSearchBox\": \n{\n    \"HintText\": \"Type Search Text\",\n    \"ToolTipText\": \"Tool Tip\",\n    \"OnTextChanged\": \"$1(%)\",\n    \"OnTextCommitted\": \"$2(%)\",\n    \"SelectAllTextWhenFocused\": false,\n    \"DelayChangeNotificationWhiteTyping\": true\n}"
        ]
    },
    "SSeparator": {
        "prefix": "SSeparator",
        "body": [
            "\"SSeparator\":\n{\n    \"Thickness\":5,\n    \"Orientation\": \"Horizontal\"\n}"
        ]
    },
    "SSlider": {
        "prefix": "SSlider",
        "body": [
            "\"SSlider\":\n{\n    \"Orientation\": \"$1\",\n    \"Value\": 0.5,\n    \"Locked\": false,\n    \"OnValueChanged\": \"print(%)\"\n}"
        ]
    },
    "SSpacer": {
        "prefix": "SSpacer",
        "body": [
            "\"SSpacer\":\n{\n    \"Size\": [100, 50]\n}"
        ]
    },
    "SSpinBox": {
        "prefix": "SSpinBox",
        "body": [
            "\"SSpinBox\": \n{\n    \"Type\": \"int\",\n    \"Value\": 10,\n    \"MinValue\": 1,\n    \"MaxValue\": 8192,\n    \"MinSliderValue\": 1,\n    \"maxSliderValue\": 8192,\n    \"Delta\": 1,\n    \"OnValueChanged\": \"print(%)\"\n}"
        ]
    },
    "SSplitter": {
        "prefix": "SSplitter",
        "body": [
            "\"SSplitter\": {\n    \"Orientation\": \"Horizontal\",\n    \"PhysicalSplitterHandleSize\": 4,\n    \"ResizeMode\": \"FixedPosition\",\n    \"Slots\": [\n        {\n            \"Value\": 0.4,\n            $1\n        },\n        {\n            \"Value\": 0.6,\n            $2\n        }\n\n    ]\n}"
        ]
    },
    "STextBlock": {
        "prefix": "STextBlock",
        "body": [
            "\"STextBlock\":\n{\n    \"Text\": \"$1\"\n}"
        ]
    },
    "STextBlock Example": {
        "prefix": "STextBlock Example",
        "body": [
            "\"STextBlock\":\n{\n    \"Text\": \"Text Content\",\n    \"ColorAndOpacity\": [0, 0.5, 1, 1],\n    \"Font\": {\n        \"Style\": \"FEditorStyle\",\n        \"StyleName\": \"FontAwesome.16\"\n    },\n    \"ShadowOffset\": [2, 2],\n    \"ShadowColorAndOpacity\": [0, 0, 0, 0.75],\n    \"Justification\": \"Left\"\n}"
        ]
    },
    "SThrobber": {
        "prefix": "SThrobber",
        "body": [
            "\"SThrobber\":\n{\n    \"NumPieces\": 3,\n    \"Animate\": \"$1\"\n} "
        ]
    },
    "STreeView": {
        "prefix": "STreeView",
        "body": [
            "\"STreeView\":\n{\n    \"Aka\": \"$1\",\n    \"SHeaderRow\": {\n        \"Columns\": [\n            {\n                \"DefaultLabel\": \"Tree View List\",\n                \"FillWidth\": 1\n            }\n        ]\n    },\n    \"TreeItemSource\": [\"ItemA\", \"ItemB\", \"ItemC\", \"ItemD\", \"ItemE\", \"ItemF\" ],\n    \"TreeItemParentIndices\": [-1, 0, 0, 2, 1, 1]\n}"
        ]
