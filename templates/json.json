    "SCheckBox Example": {
        "prefix": "SCheckBox Example",
        "body": [
            "\"SCheckBox\":\n{\n    \"CheckBoxStyle\": {\n        \"Style\": \"FCoreStyle\",\n        \"StyleName\": \"ToggleButtonCheckbox\"\n    },\n    \"ColorAndOpacity\": [1, 1, 1, 1],\n    \"Content\": {\n        \"HAlign\": \"Center\",\n        \"SBox\": {\n            \"VAlign\": \"Center\",\n            \"HAlign\": \"Center\",\n            \"Padding\" : [10, 10],\n            \"Content\":\n            {\n                \"STextBlock\":\n                {\n                    \"Text\": \"Toggle Button is actually a SCheckBox\"\n                }\n            }\n        }\n    },\n    \"ForegroundColor\": [1, 1, 0.5, 1],\n    \"HAlign\": \"Left\",\n    \"Padding\": 6,\n    \"IsChecked\": true,\n    \"OnCheckStateChanged\": \"print('checked: %'.format(%))\"\n}"
        ]
    },
    "SColorBlock Example": {
        "prefix": "SColorBlock Example",
        "body": [
            "\"SColorBlock\": \n{\n    \"Color\":[$1, $2, $3, $4],\n    \"ColorIsHSV\": false, \n    \"OnMouseButtonDown\": \"print('mouse down')\",\n    \"ShowBackgroundForAlpha\": true,\n    \"Size\": [200, 50],\n    \"UseSRGB\": true\n}"
        ]
    },
    "SComboButton Example": {
        "prefix": "SComboButton Example",
        "body": [
            "\"SComboButton\": {\n    \n    \"ButtonColorAndOpacity\": [1, 1, 1, 1],\n    \"ButtonContent\": {\n        \"STextBlock\": {\n            \"Text\": \"Choose And Trigger the Command\"\n        }\n    },\n    \"ButtonStyle\":{\n        \"Style\": \"FEditorStyle1\",\n        \"StyleName\": \"FlatButton.Default\"\n    }, \n    \"ContentPadding\": [5, 2],\n    \"ForegroundColor\": [1, 1, 1, 1],\n    \"HAlign\": \"Center\",\n    \"HasDownArrow\": true,\n    \"OnGetMenuContent\": {\n        \"items\": [\n            {\"name\": \"$1\"          , \"Command\": \"print('$2')\" },\n            {\"name\": \"$3\"          , \"Command\": \"print('$4')\" },\n            {\"name\": \"$5\"          , \"Command\": \"print('$6')\" }\n        ]\n    },\n    \"VAlign\": \"Bottom\"\n}"
        ]
    },
    "SDropTarget Example": {
        "prefix": "SDropTarget Example",
        "body": [
            "\"SDropTarget\": {\n   \"ColorAndOpacity\": [1, 1, 1, 1],\n   \"ForegroundColor\": [1, 1, 1, 1],\n   \"Text\": \"SDropTarget\",\n   \"HAlign\": \"Center\",\n   \"OnDrop\": \"$1.on_drop_func(%**kwargs)\",\n   \"Content\":\n   {             \n        \"SMultiLineEditableTextBox\": {\n            \"Margin\": [0, 20],\n            \"Text\": \"Drag And Drop Something here.\"\n        }\n   }\n}"
        ]
    },
    "SExpandableArea Example": {
        "prefix": "SExpandableArea Example",
        "body": [
            "\"SExpandableArea\": {\n    \"Aka\": \"expandable_area\",\n    \"AllowAnimatedTransition\": true,\n    \"BorderImage\": {\n        \"Style\": \"FEditorStyle\",\n        \"Brush\": \"DetailsView.CategoryTop\"\n    },\n    \"ButtonColorAndOpacity\": [1, 1, 1, 1], \n    \"Padding\": 4,\n    \"HeaderPadding\": [5, 3],\n    \"InitiallyCollapsed\": false,\n    \"HeaderContent\": {\n        \"STextBlock\": {\n            \"Text\": \"Some Header Text\"\n        }\n    },\n    \"MaxHeight\": 100,\n    \"MinWidth\": 100,\n    \"OnAreaExpansionChanged\": \"print('OnAreaExpansionChanged text: {}'.format(%))\",\n    \"BodyBorderImage\": {\n        \"Style\": \"FAppStyle\",\n        \"Brush\": \"Brushes.Recessed\"\n    },\n    \"BodyBorderBackgroundColor\": [1, 1, 1, 1],\n    \"BodyContent\": {\n        \"SHorizontalBox\": {\n            \"Slots\": [\n                {\n                    \"STextBlock\": { \"Text\": \"Some Body Text\" }\n                },\n                {\n                    \"SButton\": { \"ContentPadding\": 5, \"Text\": \"Some Button In Body Content\" }\n                }\n            ]\n        }\n    }\n}"
        ]
    },
    "SHeader Example": {
        "prefix": "SHeader Example",
        "body": [
            "\"SHeader\":\n{\n    \"Content\":\n    {\n        \"STextBlock\": {\n            \"Text\": \"Some Italic Header\",\n            \"ColorAndOpacity\": [1,1,0,2],\n            \"Justification\": \"Center\",\n            \"Font\": {\n                \"Style\": \"FCoreStyle\",\n                \"StyleName\": \"DefaultFont.Italic.12\"\n            },\n            \"ShadowOffset\": [2,2],\n            \"ShadowColorAndOpacity\": [0,0,0,1]\n        }\n    }\n}"
        ]
    },
    "SHeaderRow Example": {
        "prefix": "SHeaderRow Example",
        "body": [
            "\"SListView<MultiColumn>\": {\n    \"SHeaderRow\": {\n        \"ColorAndOpacity\": [1, 1, 0, 1],\n        \"ForegroundColor\": [1, 0, 1, 1],\n        \"ToolTipText\": \"$1\",\n        \"Columns\":\n        [\n            {\n                \"DefaultLabel\": \"Name\",\n                \"FillWidth\": 0.25\n            },\n            {\n                \"DefaultLabel\": \"A\",\n                \"FillWidth\": 0.25\n            },\n            {\n                \"DefaultLabel\": \"B\",\n                \"FillWidth\": 0.25\n            }\n        ]\n    },\n    \"ListItemsSource\": [ [\"Line 1\", \"a\", \"b\"]]\n}"
        ]
    },
    "SHorizontalBox Example": {
        "prefix": "SHorizontalBox Example",
        "body": [
            "\"SHorizontalBox\": \n{\n    \"ToolTipText\": \"Use 'AutoWidth' and 'SSpacer' to layout the to the right\",\n    \"Slots\": [\n        {\n            \"AutoWidth\": true,\n            \"Padding\": 2,\n            \"SButton\":\n            {\n                \"Text\": \"Widget At Left\",\n                \"HAlign\": \"Center\",\n                \"VAlign\": \"Center\",\n                \"ContentPadding\": 4,\n                \"ToolTipText\": \"Test\",\n                \"OnClick\": \"print('Button Test clicked.')\"\n            }\n        },\n        {\n            \"AutoWidth\": true,\n            \"SMultiLineEditableTextBox\":\n            {\n                \"Text\": \"Text Box follow the left button.\",\n                \"AutoWrapText\": true\n            }\n        },\n        {\n            \"SSpacer\":{}\n        },\n        {\n            \"AutoWidth\": true,\n            \"SButton\": { \"ContentPadding\": 4, \"Text\": \"Right\"}\n        }\n    ]\n}"
        ]
    },
    "SHyperlink Example": {
        "prefix": "SHyperlink Example",
        "body": [
            "\"SHyperlink\":\n{\n    \"ColorAndOpacity\": [1, 1, 1, 1],\n    \"Padding\": [20, 0],\n    \"Style\":\n    {\n        \"Style\": \"FEditorStyle\",\n        \"StyleName\": \"NavigationHyperlink\"\n    },\n    \"Text\": \"www.tacolour.xyz\",\n    \"OnNavigate\": \"print('link clicked.')\"\n}"
        ]
    },
    "SImage Examples": {
        "prefix": "SImage Examples",
        "body": [
            "\"SImage\": {\n    \"Aka\": \"$1\",\n    \"ColorAndOpacity\": [1, 1, 1, 1],\n    \"ImagePath\": \"../ChameleonGallery/Images/cartoon_game_map.png\",\n    \"ToolTipText\": \"'ImagePath' is a relative path with the local ui json file.\",\n    \"DesiredSizeOverride\": [128, 128],\n    \"OnMouseButtonDown\": \"print('clicked')\"\n}\n        "
        ]
    },
    "SListViewMultiColumn Example": {
        "prefix": "SListViewMultiColumn Example",
        "body": [
            "\"SListView<MultiColumn>\": {\n    \"Aka\": \"SListViewMultiColumn_B\",\n    \"EnableAnimatedScrolling\": true,\n    \"SHeaderRow\": {\n        \"Columns\":\n        [\n            {\n                \"DefaultLabel\": \"Line\",\n                \"FillWidth\": 0.25\n            },\n            {\n                \"DefaultLabel\": \"A\",\n                \"FillWidth\": 0.25\n            },\n            {\n                \"DefaultLabel\": \"B\",\n                \"FillWidth\": 0.25\n            },\n            {\n                \"DefaultLabel\": \"C\",\n                \"FillWidth\": 0.25\n            }\n        ]\n    },\n    \"ListItemsSource\": [\n        [\"Line 1\", \"<RichText.red>Red</>\", \"<RichText.green>Green</>\", \"<RichText.cyan>Blue</>\"],\n        [\"Line 2\", \"<RichText.green>Green</>\", \"<RichText.cyan>Blue</>\",\"<RichText.red>Red</>\"],\n        [\"Line 3\", \"<RichText.cyan>Blue</>\",\"<RichText.red>Red</>\", \"<RichText.green>Green</>\"]\n    ],\n    \"OnSelectionChanged\": \"print(%Index)\",\n    \"OnMouseButtonDoubleClick\": \"print('double click: {}'.format(%Index))\",\n    \"RichText\": true\n}"
        ]
    },
    "SMultiLineEditableTextBox Example": {
        "prefix": "SMultiLineEditableTextBox Example",
        "body": [
            "\"SMultiLineEditableTextBox\": {\n    \"AutoWrapText\": true,\n    \"HintText\": \"This is a SMultiLineEditableTextBox\",\n    \"Justification\": \"Center\",\n    \"LineHeightPercentage\": 1.6,\n    \"Margin\": [10, 20],\n    \"Marshaller\": \"ChameleonRichText\",\n    \"OnTextChanged\": \"print(%)\",\n    \"OnTextCommitted\": \"print('input text: {}'.format(%))\",\n    \"SelectAllTextWhenFocused\": true,\n    \"Text\": \"<TextStyle FontFamily=\\\"Roboto\\\" FontSize=\\\"15\\\" FontStyle=\\\"Regular\\\" FontColor=\\\"(R=1,G=0.5,B=0,A=1)\\\">SMultiLineEditableTextBox</>\\n<TextStyle FontFamily=\\\"Roboto\\\" FontSize=\\\"10\\\" FontStyle=\\\"Regular\\\" FontColor=\\\"(R=0.8,G=0.02,B=0,A=1)\\\">SMultiLineEditableTextBox line1</>\\nSMultiLineEditableTextBox line2\",\n    \"ToolTipText\": \"Tool Tips\"\n}"
        ]
    },
    "SMultiLineEditableText Example": {
        "prefix": "SMultiLineEditableText Example",
        "body": [
            "\"SMultiLineEditableText\": {\n    \"AutoWrapText\": true,\n    \"HintText\": \"This is a SMultiLineEditableText\",\n    \"Justification\": \"Center\",\n    \"LineHeightPercentage\": 1.6,\n    \"Margin\": [10, 20],\n    \"Marshaller\": \"ChameleonRichText\",\n    \"OnTextChanged\": \"print(%)\",\n    \"OnTextCommitted\": \"print('input text: {}'.format(%))\",\n    \"SelectAllTextWhenFocused\": true,\n    \"Text\": \"<TextStyle FontFamily=\\\"Roboto\\\" FontSize=\\\"15\\\" FontStyle=\\\"Regular\\\" FontColor=\\\"(R=0,G=0.5,B=1,A=1)\\\">SMultiLineEditableText</>\\n<TextStyle FontFamily=\\\"Roboto\\\" FontSize=\\\"10\\\" FontStyle=\\\"Regular\\\" FontColor=\\\"(R=0.8,G=0.02,B=0,A=1)\\\">is similar with </>\\nSMultiLineEditableTextBox\\n which is not a 'Box'\",\n    \"ToolTipText\": \"Tool Tips\"\n}"
        ]
    },
    "SOverlay Example": {
        "prefix": "SOverlay Example",
        "body": [
            "\"SOverlay\": {\n    \"Slots\":\n    [\n        {\n            \"SImage\": {\n                    \"Aka\": \"ImageBackgroundInSOverlay\",\n                    \"ImagePathInPlugin\": \"Resources/PythonIcon_128x.png\"\n            }\n        },\n        {\n            \"SUniformGridPanel\": {\n                \"Slots\": [\n                    {\n                        \"Column_Row\": [0, 0],\n                        \"SButton\": { \"Text\": \"Tile 0_0\", \"ButtonColorAndOpacity\": [1, 1, 1, 0.5] }\n                    },\n                    {\n                        \"Column_Row\": [1, 0],\n                        \"SButton\": { \"Text\": \"Tile 1_0\", \"ButtonColorAndOpacity\": [1, 1, 1, 0.5] }\n                    },\n                    {\n                        \"Column_Row\": [2, 0],\n                        \"SButton\": { \"Text\": \"Tile 2_0\", \"ButtonColorAndOpacity\": [1, 1, 1, 0.5] }\n                    },\n                    {\n                        \"Column_Row\": [0, 1],\n                        \"SButton\": { \"Text\": \"Tile 0_1\", \"ButtonColorAndOpacity\": [1, 1, 1, 0.5] }\n                    },\n                    {\n                        \"Column_Row\": [1, 1],\n                        \"SButton\": { \"Text\": \"Tile 1_1\", \"ButtonColorAndOpacity\": [1, 1, 1, 0.5]}\n                    },\n                    {\n                        \"Column_Row\": [2, 1],\n                        \"SButton\": { \"Text\": \"Tile 2_1\", \"ButtonColorAndOpacity\": [1, 1, 1, 0.5]}\n                    }\n                ]\n            }\n        }\n    ]\n}"
        ]
    },
